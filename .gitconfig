.gitconfig
[user]
        email = lasse.grotell@gmail.com
        name = Lars Grotell

[push]
  default = upstream
  autoSetupRemote = true

[alias]
        co = checkout
        br = branch
        ci = commit
        st = status

[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
